{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/url/:shortId",
      "dest": "/url/:shortId"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "MONGODB_CONNECT_URI": "your_mongodb_uri_here"
  }
}
